
@{
    ViewData["Title"] = "InsertImage";
}

<h2>InsertImage</h2>

<form method="post" enctype="multipart/form-data" asp-controller="Image" asp-action="InsertImage">
    <div class="form-group">
        <div class="col-md-8">
            <p>Upload files:</p>
            <input type="file" name="files" multiple />
        </div>
    </div>
    <div class="form-group">
        <div class="col-md-1">
            <input type="submit" value="Upload" />
        </div>
    </div>
</form>
